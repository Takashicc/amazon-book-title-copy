{
  "name": "amazon-book-title-copy",
  "description": "Copying amazon book titles with a button",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/*"
      ],
      "js": [
        "background.js"
      ]
    }
  ]
}
